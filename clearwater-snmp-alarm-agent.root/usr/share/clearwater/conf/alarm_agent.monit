# Monitor the service's PID file.
check process alarm_agent_process with pidfile /var/run/cw_alarm_agent.pid
  group snmp

  start program = "/etc/init.d/clearwater-snmp-alarm-agent start"
  stop program = "/etc/init.d/clearwater-snmp-alarm-agent stop"
  restart program = "/etc/init.d/clearwater-snmp-alarm-agent restart"

